[org/gnome/desktop/a11y/keyboard]
togglekeys-enable=true

[org/gnome/desktop/interface]
color-scheme='prefer-dark'
enable-hot-corners=false
gtk-enable-primary-paste=false
show-battery-percentage=true

[org/gnome/desktop/input-sources]
xkb-options=['caps:none']

[org/gnome/desktop/notifications]
show-in-lock-screen=false

[org/gnome/desktop/peripherals/touchpad]
disable-while-typing=true
tap-to-click=true

[org/gnome/desktop/privacy]
old-files-age=uint32 14
remember-recent-files=false
remove-old-trash-files=true
remove-old-temp-files=true

[org/gnome/desktop/screensaver]
restart-enabled=true

[org/gnome/desktop/search-providers]
disabled=['org.gnome.Software.desktop', 'org.gnome.Terminal.desktop', 'org.gnome.Boxes.desktop', 'org.gnome.Contacts.desktop', 'org.mozilla.firefox.desktop']

[org/gnome/desktop/wm/preferences]
button-layout='appmenu:maximize,close'

[org/gnome/desktop/wm/keybindings]
close=['<Super>q']
minimize=['<Super>d']

[org/gnome/mutter]
center-new-windows=true
check-alive-timeout=15000
workspaces-only-on-primary=false

[org/gnome/settings-daemon/plugins/power]
sleep-inactive-ac-type='suspend'

[org/gnome/shell]
favorite-apps=['firefox.desktop', 'org.gnome.Terminal.desktop', 'org.gnome.Nautilus.desktop']

[org/gnome/shell/extensions/privacy-menu]
click-to-toggle=true

[org/gnome/shell/extensions/quick-settings-tweaks]
datemenu-remove-notifications=false
notifications-enabled=false

[org/gnome/system/location]
enabled=false
